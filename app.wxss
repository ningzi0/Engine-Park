/**app.wxss**/
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 

.f-l{
  float: left;
}
.f-r{
  float: right;
}
.clearfix:after{
  clear: both;
  height: 0;
  content: "";
  line-height: 0;
  font-size: 0;
  display: block;
}


.back-pre{
  position: fixed;
  top: 5.5vh;
  left: 1em;
  width: 1.5em;
  height: 1.5em;
  z-index: 55;
}

.pd7{
  padding-bottom: 7em;
}


::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}