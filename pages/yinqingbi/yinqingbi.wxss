page{
  background-color: #f5f5f5;
}
.yinqingbiTop{
  background-color: #efac37;
  padding: 1em;
}
.yinqingbiTtitle text{
  color: rgba(255,255,255,0.8);
  font-size: 0.8em;
}
.yinqingbiNum{
  font-size: 3.5em;
  font-weight: bold;
  color: #fff;
  margin-top: 0.25em;
}
.tips{
  font-size: 0.75em;
  padding: 1em;
  color: rgb(182, 182, 182);
}
.yinqingbiMonth{
  padding: 1em;
}
.yinqingbiMonth text{
  font-size: 0.6em;
  display: inline-block;
  vertical-align: middle;
}

.yinqingbiItemCon {
  background-color: #fff;
  padding: 1em;
  border-bottom: 1px solid #eee;
}
.yinqingbiItemCon .yinqingbiLeftP{
  font-size: 0.95em;
  color: #898989;
  margin-top: 0.35em;
}
.yinqingbiLeft{
  display: inline-block;
  width: 80%;
}
.yinqingbiRight{
  display: inline-block;
  vertical-align: top;
  font-size: 1.1em;
  font-weight: bold;
}





